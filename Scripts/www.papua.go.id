    function getMenuItem(data) {
        var cls = "";
        var a_attr = "";
        //console.log(data.level+"=>"+data.link);         
        if(data.level == 1){
            if(data.sub != ''){
                cls = 'class="dropdown"';
                //a_attr = 'class="dropdown-toggle" data-toggle="dropdown"';
                //a_attr = 'class="dropdown"';
            }
        } else if(data.level > 1){
            if(data.sub != ''){
                cls = 'class="dropdown"';
                a_attr = 'tabindex="-1"';
            }
        }
        var link ="";
        if(data.parent== 0 && (data.link.indexOf("http://") == 0 || data.link.indexOf("https://") == 0)){
            link = data.link;
            //$('a[href]').attr('target', '_blank');
        }else
        if(data.parent == 7){
            link = "http://"+data.link;                    
        } else link = "https://www.papua.go.id/"+data.link;   

        var item = $("<li "+cls+">").append($("<a "+a_attr+" href='"+link+"'>"+data.name+"</a>"));

        if(data.sub){            
            var subList = $("<ul>");
            $.each(data.sub, function () {
                subList.append(getMenuItem(this));
            });
            item.append(subList);
        }

        return item;
    }
</script>


